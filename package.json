{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "total-build": "yarn run postcss && yarn run build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules && rm -f next-env.d.ts && rm -rf yarn.lock && rm -rf test-scripts & rm -rf styles",
    "env-pull": "vercel env pull .env.development.local",
    "generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "migrate": "drizzle-kit push:pg --config=drizzle.config.ts",
    "drizzle-studio": "drizzle-kit studio --config=drizzle.config.ts",
    "test": "jest",
    "postcss": "postcss src/css/globals.css -o styles/globals.css"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.26.2",
    "@heroicons/react": "^2.0.18",
    "@neondatabase/serverless": "^0.6.0",
    "@tanstack/react-query": "^4.18.0",
    "@vercel/postgres": "^0.5.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "inversify": "^6.0.2",
    "next": "13.5.6",
    "react": "^18",
    "react-dom": "^18",
    "react-modern-drawer": "^1.2.2",
    "tsv": "^0.2.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.40",
    "@types/jest": "^29.5.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/tsv": "^0.2.4",
    "autoprefixer": "^10",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "fs": "^0.0.1-security",
    "jest": "^29.7.0",
    "pg": "^8.11.3",
    "postcss": "^8",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.1",
    "tailwindcss": "^3.3.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5",
    "vercel": "^32.5.0"
  }
